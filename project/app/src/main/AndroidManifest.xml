<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lukasz.ecodrive">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Activities.Start" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Information"/>
        <activity android:name=".Activities.Login"/>
        <activity android:name=".Activities.Registration1"/>
        <activity android:name=".Activities.Registration2"/>
        <activity android:name=".Activities.Learn"/>
        <activity android:name=".Activities.Bluetooth"/>
        <activity android:name=".Activities.TestMeasurement"/>
        <activity android:name=".Activities.Level"/>
        <activity android:name=".Activities.Measurement"/>
        <activity android:name=".Activities.EndLevel"/>

        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
